{
    "compilerOptions": {
        "module": "commonjs",
        "declaration": true,
        "noImplicitAny": false,
        "removeComments": true,
        "noLib": false,
        "allowSyntheticDefaultImports": true,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "preserveSymlinks": true,
        "target": "esnext",
        "sourceMap": true,
        "outDir": "./dist",
        "baseUrl": "./src",
        "paths": {
        },
        "noUnusedLocals": false,
        "noUnusedParameters": false,
        "newLine": "LF"
    }
}

