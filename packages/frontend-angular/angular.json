{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "version": 1,
    "newProjectRoot": "projects",
    "projects": {
        "frontend-angular": {
            "projectType": "library",
            "root": "src",
            "sourceRoot": "src",
            "prefix": "lib",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-ng-packagr:build",
                    "options": {
                        "tsConfig": "tsconfig.json",
                        "project": "ng-package.json"
                    },
                    "configurations": {
                        "production": {
                            "tsConfig": "tsconfig.json"
                        }
                    }
                }
            }
        }
    },
    "defaultProject": "frontend-angular"
}
